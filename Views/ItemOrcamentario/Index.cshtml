@Model ProjetoBudget.Models.itemOrcamentario
@{
    ViewBag.Title = "Listagem Orçamentos";
}
<h2 class="tituloPcad"> Itens Orçamentarios cadastrados: </h2>
<table class="table">
    <thead>
        <tr class="tablecad">
            <th scope="col"> Descrição item</th>
            <th scope="col"> Tipo Gasto  </th>
            <th scope="col"> Tipo Item:  </th>
            <th scope="col"> Valor Unitario </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            @foreach (var item in Model)
            {
                <td>@item.descricaoItem</td>
                <td>@item.tipoGasto</td>
                <td>@item.TipoItem.nomeTipoItem</td>
                <td>@item.valorUnitario</td>
                

            }
        </tr>
    </tbody>
</table>

